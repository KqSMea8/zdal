	<bean id="zdalZoneDsZoneErrorRight2_users" class="com.alipay.zdal.rule.config.beans.TableRule"
		init-method="init">
		<property name="logicTableName" value="users" />
		<property name="tbSuffix">
			<value>resetForEachDB:[_0-_4]</value>
